
SPATH       := $($(HELLOWORLD)_SRC_PATH)
DEP_FILES   := $(SONIC_COMMON_FILES_LIST)   
DEP_FILES   += $(SONIC_COMMON_BASE_FILES_LIST)
SMDEP_FILES := $(addprefix $(SPATH)/,$(shell cd $(SPATH) && git ls-files))

$(HELLOWORLD)_CACHE_MODE  := GIT_CONTENT_SHA 
$(HELLOWORLD)_DEP_FLAGS   := $(SONIC_COMMON_FLAGS_LIST)
$(HELLOWORLD)_DEP_FILES   := $(DEP_FILES)
$(HELLOWORLD)_SMDEP_FILES := $(SMDEP_FILES)
$(HELLOWORLD)_SMDEP_PATHS := $(SPATH)